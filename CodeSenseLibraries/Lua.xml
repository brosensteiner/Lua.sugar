<codesense version="1.0">

	<behavior>
		<append-dynamic>
			<matched-suffix>((\s*)(\(|=))|\s*[^(=\s]?</matched-suffix>
			<transform-into>(?1:$1:\( $1 \) $0)</transform-into>
    	</append-dynamic>
    	<confirm characters=")" />
	</behavior>
    
	
